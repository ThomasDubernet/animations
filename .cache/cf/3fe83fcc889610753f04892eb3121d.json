   top: 50%;\n      left: 50%;\n      cursor: pointer;\n\n      transform: translate3d(-50%, -50%, 0);\n\n      .burger {\n        position: relative;\n        display: inline-block;\n        height: 100%;\n        width: 100%;\n\n        $offsetY: 6px;\n        .bar {\n          background: black;\n          position: absolute;\n          left: 0;\n          height: 2px;\n          width: $size;\n          top: 50%;\n          opacity: 0.7;\n          transition: transform 500ms cubic-bezier(.48,.01,.65,.99);\n\n        }\n        .bar-1{\n          transform: translate3d(0, -$offsetY, 0);\n        }\n        .bar-2{\n          transform: translate3d(0, $offsetY, 0);\n        }\n\n      }\n\n      &.active {\n        .bar{\n          opacity: 0.9;\n        }\n        &:hover {\n          .bar-1 {\n            transform: translate3d(0, 50%, 0) rotate(-135deg);\n          }\n          .bar-2 {\n            transform: translate3d(0, 50%, 0) rotate(-45deg);\n          }\n        }\n        .bar-1 {\n          transition: transform 600ms cubic-bezier(.48,.01,.65,.99);\n          transform: translate3d(0, 50%, 0) rotate(-315deg);\n        }\n        .bar-2 {\n          transition: transform 600ms cubic-bezier(.48,.01,.65,.99);\n          transform: translate3d(0, 50%, 0) rotate(-225deg);\n        }\n      }\n    }\n  }\n}\n\n.menu {\n  position: fixed;\n  margin-top: 100px;\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  text-align: center;\n  opacity: 0;\n  transform: translate3d(0, 100px, 0);\n  transition: transform 1200ms ease, opacity 1200ms ease;\n\n  &.active {\n    opacity: 1;\n    transform: translate3d(0, 0, 0);\n  }\n\n  .menu-item {\n    font-size: 65px;\n    text-transform: uppercase;\n    margin: 8px 0;\n    cursor: pointer;\n\n    $transitionTime: 200ms;\n    span.item {\n      display: inline-block;\n      margin: 0 2px;\n      transform-origin: left bottom;\n      transition: transform $transitionTime, opacity $transitionTime;\n      opacity: 0.9;\n\n      @for $i from 1 through 10 {\n        $m: calc(10 - #{$i});\n        &:nth-child(#{$i}) {\n            transition-delay: calc(#{$m}*#{$transitionTime});\n        }\n      }\n    }\n\n    &:hover {\n      span.item{\n        transform: translate3d(0, 0, 0) rotate(12deg);\n        opacity: 1;\n\n        @for $i from 1 through 10 {\n          &:nth-child(#{$i}) {\n            transition-delay: calc((#{$i}-1)*#{$transitionTime});\n          }\n        }\n      }\n    }\n  }\n}\n\n.main-container {\n  margin: 0 3rem;\n  transition:transform 1250ms ease, opacity 1200ms ease;\n\n  &.hidden {\n    transform: translate3d(0, 25%, 0);\n    opacity: 0;\n  }\n}"},"lineCount":null}},"error":null,"hash":"d828e95fcbac8959f8b15fed40412cb2","cacheData":{"env":{}}}